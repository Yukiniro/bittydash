{
  "name": "tinydash",
  "version": "0.0.1",
  "description": "A small JavaScript utility library",
  "main": "index.ts",
  "scripts": {
    "lint": "npx eslint src/**/*.{js,ts} --fix",
    "prettier": "npx prettier src/**/*.{js,ts} --write",
    "build": "npx rollup -c rollup.config.js",
    "test": "npx vitest",
    "test:run": "npx vitest run",
    "test:coverrage": "npx vitest --coverage",
    "publish": "npm publish"
  },
  "keywords": [
    "vite",
    "vitest",
    "docusaurus",
    "template"
  ],
  "author": "Yukiniro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukiniro/tinydash"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@yukiniro/eslint-config": "^0.0.6",
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "prettier-eslint": "^13.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vitest": "^0.7.6"
  }
}
