{
  "name": "bittydash",
  "version": "0.0.8",
  "description": "A small JavaScript utility library",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint src/**/*.{js,ts} --fix",
    "lint:test": "npx eslint __test__/**/*.{js,ts} --fix",
    "prettier": "npx prettier src/**/*.{js,ts} --write",
    "prettier:test": "npx prettier __test__/**/*.{js,ts} --write",
    "doc": "npx typedoc",
    "build:dev": "npx rollup -c rollup.config.dev.js",
    "build:prod": "npx rollup -c rollup.config.prod.js",
    "test": "npx vitest",
    "test:run": "npx vitest run",
    "test:coverrage": "npx vitest --coverage",
    "publish": "npm run build:dev && npm run build:prod && npm publish"
  },
  "keywords": [
    "utility",
    "util",
    "modules",
    "lodash"
  ],
  "files": [
    "dist"
  ],
  "author": "Yukiniro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukiniro/bittydash"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@yukiniro/eslint-config": "^0.0.6",
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "prettier-eslint": "^13.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2",
    "vitest": "^0.7.6"
  }
}
